public class Solution {
    public string GenerateTheString(int n) {
        
        string result = "";
        if(n % 2 == 0)
        {
            for(int i = 0;i<n-1;i++)
            {
                result += 'a';
            }
            result += 'b';
        }
        else
        {
            for(int i = 0;i<n;i++)
            {
                result += 'a';
            }
        }
        
        return result;
    }
}
